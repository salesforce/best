{
  "name": "@best/frontend",
  "version": "0.0.7",
  "private": true,
  "description": "Best frontend",
  "scripts": {
    "clean": "rimraf public/assets/js",
    "eslint": "eslint src/",
    "flow": "flow --show-all-errors",
    "lint": "yarn run eslint && yarn run flow",
    "serve": "node server/index.js",
    "start": "yarn build:prod && node server/index.js",
    "serve:watch": "nodemon --watch public --watch server server/index.js",
    "build": "cross-env NODE_ENV=development rollup -c",
    "build:prod": "cross-env NODE_ENV=production rollup -c",
    "watch": "concurrently \"yarn run build --watch\" \"yarn run serve:watch\"",
    "deploy": "yarn run build-prod && gh-pages -d .",
    "test": "jest"
  },
  "keywords": [
    "lwc",
    "benchmark",
    "perf",
    "testing"
  ],
  "author": "LWC Team",
  "homepage": "https://github.com/salesforce/best",
  "repository": {
    "type": "git",
    "url": "https://github.com/salesforce/best"
  },
  "dependencies": {
    "apicache": "^1.2.0",
    "express": "^4.16.2",
    "memoizee": "^0.4.11",
    "@best/store-aws": "0.0.7"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.15.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-lwc": "^0.2.4",
    "lwc-compiler": "0.17.14",
    "lwc-engine": "0.17.14",
    "rimraf": "^2.6.2",
    "rollup": "~0.53.0",
    "nodemon": "~1.14.12",
    "rollup-plugin-lwc-compiler": "0.17.14",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "uglify-es": "3.3.7"
  }
}
